<h2><%= I18n::t('users.show.session.title') %></h2>

<div class="entrybody">
  <dl class="list">
    <dt>Name:</dt>
    <dd><%= h @user.name %></dd>

    <dt>Email:</dt>
    <dd><%= h @user.email %></dd>

    <dt>Login count:</dt>
    <dd><%= h @user.login_count %></dd>

    <dt>Last request at:</dt>
    <dd><%= h @user.last_request_at %></dd>

    <dt>Last login at:</dt>
    <dd><%= h @user.last_login_at %></dd>

    <dt>Current login at:</dt>
    <dd><%= h @user.current_login_at %></dd>

    <dt>Last login IP:</dt>
    <dd><%= h @user.last_login_ip %></dd>

    <dt>Current login IP:</dt>
    <dd><%= h @user.current_login_ip %></dd>
  </dl>
</div>
